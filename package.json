{
  "name": "stocker",
  "version": "1.0.0",
  "description": "An app to see stock prices",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "watch": "nodemon server/server.js",
    "start:prod": "node server/server.js",
    "start:dev": "concurrently \"nodemon server/server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yukezter/stocker.git"
  },
  "author": "Bryan Hinchliffe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yukezter/stocker/issues"
  },
  "homepage": "https://github.com/Yukezter/stocker#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "connect-flash": "^0.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-promise-router": "^3.0.3",
    "express-session": "^1.16.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.5.8",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "socketio-jwt": "^4.5.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
